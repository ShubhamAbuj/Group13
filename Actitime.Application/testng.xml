<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count = "3" parallel = "classes">
	<listeners>
		<listener class-name="extendReport.ExtentTestNGIReporterListener" />
	</listeners>
	
<test name="Chrome">
  <parameter name="browser" value="Chrome" />
    <classes>
      <class name="TestClass.validateHeader"/>
      <class name="TestClass.ValidateLogin"/>
      <class name="TestClass.ValidateTimetrackSUb"/>
    </classes>
</test>
  
  <test name="Opera">
  <parameter name="browser" value="Opera" />
    <classes>
      <class name="TestClass.validateHeader"/>
      <class name="TestClass.ValidateLogin"/>
      <class name="TestClass.ValidateTimetrackSUb"/>
    </classes>
  </test> 
  
  <test name="Firefox">
  <parameter name="browser" value="Firefox" />
    <classes>
      <class name="TestClass.validateHeader"/>
      <class name="TestClass.ValidateLogin"/>
      <class name="TestClass.ValidateTimetrackSUb"/>
    </classes>
</test>
</suite>
